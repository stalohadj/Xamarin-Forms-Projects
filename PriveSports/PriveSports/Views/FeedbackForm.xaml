          <StackLayout Spacing="0" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
               <ContentView ControlTemplate="{StaticResource GeneralTemplateForPopups}" Padding="20,20,20,20" HorizontalOptions="FillAndExpand" VerticalOptions="Center">
                    <Grid Grid.Row="1" HeightRequest="550" BackgroundColor="White" HorizontalOptions="FillAndExpand" VerticalOptions="Center">
                         <Label x:Name="labelLoading" Text="Loading..." IsVisible="false" />
                        <WebView HeightRequest="760" WidthRequest="210" x:Name="myWebView"  Source="https://forms.office.com/Pages/ResponsePage.aspx?id=DQSIkWdsW0yxEjajBLZtrQAAAAAAAAAAAAZAANVpvzBUOVFRRktBRTRDSFFFUk5KWkM5MTBYRVNMRi4u"/>
                    </Grid>
       
       
                       </ContentView>
                <Button HeightRequest="45" WidthRequest="250"
                        Clicked="OnClose"
                        Text="Close"/>
            </StackLayout>
